<Page x:Class="WpfBigPackApp.Views.Pages.ManagerPages.ManagerMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfBigPackApp.Views.Pages.ManagerPages"
      FontFamily="Verdana"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ManagerMainPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Агенты" Name="AgentDataBtn" Click="AgentDataBtn_Click" Width="250"></Button>
            <Button Content="Материалы" Name="MaterialsDataBtn" Click="MaterialsDataBtn_Click" Margin="0,5"></Button>
            <Button Content="Поставщики" Name="SuppliersDataBtn" Click="SuppliersDataBtn_Click"></Button>
            <Button Content="Заявки" Name="ApplicationDataBtn" Click="ApplicationDataBtn_Click" Margin="0,5"></Button>
        </StackPanel>
    </Grid>
</Page>
